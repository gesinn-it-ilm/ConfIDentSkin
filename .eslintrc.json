{
	"root": true,
	"parserOptions": {
		"ecmaVersion": 6
	},
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"node": true,
		"qunit": true
	},
	"rules": {
		"space-in-parens": "off",
		"array-bracket-spacing": "off",
		"no-use-before-define": "off",
		"mediawiki/class-doc": "off",
		"no-jquery/no-sizzle": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-constructor-attributes": "off",
		"curly": "off",
		"quotes": "off",
		"no-restricted-syntax": "off",
		"no-shadow": "off"
	}
}
